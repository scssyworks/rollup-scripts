    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-yaml": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "browserslist": "^4.24.3",
    "chalk": "^5.3.0",
    "eslint": "^9.17.0",
