    "jest": "^29.7.0",
    "rollup": "^4.28.1",
    "rollup-scripts-utils": "^0.1.1",
    "typescript": "^5.7.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
